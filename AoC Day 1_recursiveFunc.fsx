let input = inputStr.ToCharArray() |> Array.toList

let locateSanta steps =
    let rec locateSanta' (cur, basementReachedStep) step steps' =
        match steps' with
        | [] -> (cur, basementReachedStep)
        | h :: t when h = '(' -> 
            locateSanta' (cur + 1, basementReachedStep) (step + 1) t
        | h :: t when h = ')' ->
            if (cur = 0) 
                then 
                    match basementReachedStep with
                    | Some _ -> locateSanta' (cur - 1, basementReachedStep) (step + 1) t
                    | None -> locateSanta' (cur - 1, Some (step)) (step + 1) t
                else                    
                    locateSanta' (cur - 1, basementReachedStep) (step + 1) t
        | _ -> failwith "shouldn't get here"
            
    locateSanta' (0, None) 1 steps

match (locateSanta input) with
| level, Some step ->
    printf "Santa finishes at level %d and first enters the basement at step %d"  level step
| level, None ->
    printf "Santa finishes at level %d and never enters the basement"  level
